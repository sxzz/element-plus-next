{
  "name": "@element-plus-next/build",
  "version": "3.0.0-alpha.3",
  "description": "Build Toolchain for Element Plus.",
  "homepage": "https://element-plus.org/",
  "keywords": [
    "element-plus",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "build",
    "core"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus.git"
  },
  "bugs": {
    "url": "https://github.com/element-plus/element-plus/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "dev": "./src/config.ts",
      "import": "./dist/config.js"
    },
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "module:dev": "./src/index.ts",
  "scripts": {
    "build": "ep-cli build",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "@element-plus-next/node-constants": "workspace:*",
    "@element-plus-next/node-utils": "workspace:*",
    "@element-plus-next/workspace": "workspace:*",
    "esbuild": "^0.14.34",
    "local-pkg": "^0.4.1",
    "tsup": "npm:@sxzz/tsup@^6.1.2",
    "unconfig": "^0.3.4",
    "unplugin-vue": "^2.2.1"
  },
  "devDependencies": {
    "tsx": "^3.4.3",
    "vue": "^3.2.37"
  }
}
