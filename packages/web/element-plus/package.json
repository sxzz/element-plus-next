  "scripts": {
    "build": "ep-cli build",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@element-plus-next/constants": "workspace:*",
    "@element-plus-next/vue-components": "workspace:*"
  },
  "devDependencies": {
    "vue": "^3.2.31"
  }
