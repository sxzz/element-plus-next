{
  "name": "@element-plus-next/vue-components",
  "version": "3.0.0-alpha.4",
  "description": "Vue component for Element Plus.",
  "keywords": [
    "element-plus",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "components"
  ],
  "homepage": "https://element-plus.org/",
  "bugs": {
    "url": "https://github.com/element-plus/element-plus/issues"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element-plus/element-plus.git"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "ep-cli build",
    "build:types": "pnpm vue-tsc -p tsconfig.build.json",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@element-plus-next/constants": "workspace:*",
    "@element-plus-next/utils": "workspace:*",
    "@element-plus-next/vue-hooks": "workspace:*",
    "@element-plus-next/vue-utils": "workspace:*",
    "@element-plus/icons-vue": "^2.0.9",
    "@vueuse/core": "^9.1.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.0.2",
    "csstype": "2",
    "typescript": "^4.7.4",
    "vue": "^3.2.31",
    "vue-tsc": "^0.40.1"
  }
}
