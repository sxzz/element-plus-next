{
  "name": "@element-plus-next/vue-components",
  "version": "3.0.0-alpha.4",
  "description": "Vue component for Element Plus.",
  "keywords": [
    "element-plus",
    "element",
    "component library",
    "ui framework",
    "ui",
    "vue",
    "components"
  ],
  "homepage": "https://element-plus.org/",
