    "memoize-one": "^6.0.0",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@vue/runtime-dom": "^3.2.37",
    "@vue/shared": "^3.2.31",
    "@vue/test-utils": "^2.0.2",
